@page "/setting"
@inject HttpClient Http
@using BlazorDeployDemo.Shared

<h3>Setting - Overview</h3>
<PageTitle>Setting - Overview</PageTitle>

<span>My value: @setting</span>

@code {

    private string setting = "Test";

    protected override async Task OnInitializedAsync()
    {
        var result = await Http.GetFromJsonAsync<BlazorDeployDemo.Shared.Setting[]>("Setting");

        setting = result[0].Value;
    }
}
